<p style="color: green"><%= notice %></p>

<h1>Subjects</h1>

<div id="subjects">
  <% @subjects.each do |subject| %>
    <%= render subject %>
    <p>
      <%= link_to "Show this subjects", subject %>
    </p>
  <% end %>
</div>

<% if current_user.admin? %>
<%= link_to "New subjects", new_subject_path %>
<% end %>